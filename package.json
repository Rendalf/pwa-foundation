    "@types/react": "^16.3.8",
    "@types/react-dom": "^16.0.5",
    "@types/webpack-env": "^1.13.6",
    "app-manifest-loader": "^1.0.1",
    "autoprefixer": "^8.4.1",
    "babel-loader": "^8.0.0-beta.2",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-loader": "^1.0.0-alpha.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "postcss": "^6.0.22",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^4.1.1",
    "serviceworker-webpack-plugin": "^1.0.0-alpha02",
    "style-loader": "^0.20.3",
    "ts-loader": "^4.1.0",
    "typed-css-modules": "^0.3.2",
    "typescript": "^2.8.2",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "lodash": "^4.17.21",
    "minimist": "^1.2.0",
    "react": "^16.3.1",
