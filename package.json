{
  "name": "pwa",
  "version": "0.0.1",
  "description": "pwa experiment",
  "author": "rendalf <ar.lipatov93@yandex.ru>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config ./configs/webpack.dev.js",
    "dev-server": "nodemon --watch ./server --watch ./configs ./server/app.js --port=3002",
    "build": "webpack --config ./configs/webpack.prod.js",
    "server": "node ./server/app.js",
    "tcm": "tcm src",
    "tcmw": "tcm src --watch",
    "tcmc": "find src -name \"*.css.d.ts\" -type f -delete",
    "postinstall": "yarn tcm"
  },
  "sideEffects": [
    "*.css"
  ],
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/plugin-syntax-jsx": "^7.0.0-beta.44",
