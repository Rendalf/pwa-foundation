{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "strict": true,
    "module": "esnext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipLibCheck": true,
    "jsxFactory": "React.createElement",
    "lib": [
      "es2015", "dom",
    ],
  },
}
