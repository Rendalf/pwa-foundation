{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "strict": true,
    "module": "esnext",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": [
      "es2015", "dom",
    ],
  },
  "includes": [
    "./node_modules/@types/webpack-env/index.d.ts"
  ],
}
