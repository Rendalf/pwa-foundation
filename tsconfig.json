{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "../public/js",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "strict": true,
    "module": "es6",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es5",
    "jsx": "react",
    "sourceMap": true,
    "skipLibCheck": true,
  },
}
